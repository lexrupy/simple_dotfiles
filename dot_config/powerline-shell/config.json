{
  "segments": [
    "time",
    "~/.bash/custom_powerline_shell/s_virtualenv.py",
    "ssh",
    "cwd",
    "git",
    "root"
  ],
  "mode": "flat",
  "theme": "~/.bash/custom_powerline_shell/theme.py",
  "cwd": {
    "mode": "dironly"
  },
  "vcs": {
    "show_symbol": false
  },
  "time": {
    "format": "%H:%M"
  }
}
